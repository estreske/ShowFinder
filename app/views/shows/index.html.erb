<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>

 <script type="text/javascript">
   window.onload=function(){
        handler = Gmaps.build('Google', { markers: { clusterer: {gridSize: 40,
            maxZoom: 1,}}});
        handler.buildMap({ provider: { center: new google.maps.LatLng(<%= @center_lat %>,<%= @center_long %>), zoom: <%= @zoom %>  }, internal: {id: 'map_large'}}, function(){
          markers = handler.addMarkers(<%= raw @hash.to_json %>);
        });
      };
  </script>

<div class="white_title">
 <h1>All Shows</h1>
</div>

<div class="white_bg">
  <div class= "large-8 large-centered columns" id="map_large"></div>
</div>